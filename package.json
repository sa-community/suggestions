{
	"name": "sa-suggestions",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:migrate": "drizzle-kit migrate",
		"db:generate": "drizzle-kit generate --name sa-suggestions",
		"db:push": "drizzle-kit push --force",
		"db:start": "docker compose up",
		"db:studio": "drizzle-kit studio",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "eslint . && prettier --check .",
		"preview": "vite preview",
		"start": "concurrently \"pnpm dev\" \"pnpm db:start\" \"pnpm db:studio\" --kill-others",
		"uppies": "pnpm up --latest"
	},
	"dependencies": {
		"@oslojs/crypto": "1.0.1",
		"@oslojs/encoding": "1.1.0",
		"@sveltejs/adapter-auto": "^6.0.1",
		"arctic": "3.7.0",
		"bottleneck": "2.19.5",
		"compressorjs": "^1.2.1",
		"drizzle-orm": "0.44.2",
		"ioredis": "^5.6.1",
		"svelte-select": "5.8.3",
		"wrangler": "^4.20.0"
	},
	"devDependencies": {
		"@eslint/js": "9.29.0",
		"@sveltejs/kit": "2.21.5",
		"@sveltejs/vite-plugin-svelte": "5.1.0",
		"@types/node": "^24.0.1",
		"concurrently": "9.1.2",
		"drizzle-kit": "0.31.1",
		"eslint": "9.29.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-svelte": "3.9.2",
		"globals": "16.2.0",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.4.0",
		"svelte": "5.34.3",
		"svelte-check": "4.2.1",
		"typescript": "5.8.3",
		"typescript-eslint": "8.34.0",
		"vite": "6.3.5",
		"vite-plugin-devtools-json": "^0.2.0"
	}
}
